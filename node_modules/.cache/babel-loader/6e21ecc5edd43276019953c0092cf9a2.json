s.errors.push(error);\n      });\n  },\n  \n  methods: {\n    findCharacters: function() {\n      this.showSpinner = true;\n      this.results = null;\n      axios\n        .get(\n          \"https://www.anapioficeandfire.com/api/characters?page=1&pageSize=50\",\n          {\n            params: {\n              gender: this.gender\n            }\n          }\n        )\n        .then(response => {\n          this.showSpinner = false;\n          this.results = response.data;\n        })\n        .catch(error => {\n          this.showSpinner = false;\n          this.messages.push({\n            type: \"error\",\n            text: error.message\n          });\n        });\n    }\n  }\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh1,\nh2 {\n  font-weight: normal;\n}\nh2.Name {\n  \n  font-weight: Bold;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n\n  padding: 10px;\n}\n.label {\n  font-weight: bold;\n}\n</style>\n\n"],"sourceRoot":"src/views"}]}