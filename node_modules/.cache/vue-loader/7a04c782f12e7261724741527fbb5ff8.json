n\n        <p>\n          <button type=\"submit\">Search</button>\n        </p>\n      </form>\n    </div>\n    <!--<div class=\"breed-search\">\n      <form v-on:submit.prevent=\"getBreed\">\n      <p>\n       <label for=\"breed\">Select a Breed:\n         <select id=\"breed\" v-model=\"breed\">\n         <input type=\"text\" v-model=\"query\" placeholder=\"Pug\" />\n        <button type=\"submit\">Search</button>\n        </select>\n        </label>\n      </p>\n    </form> \n\n     <img v-bind:src=\"'http://openweathermap.org/img/w/' + weatherSummary.icon + '.png'\" v-bind:alt=\"weatherSummary.main\"> \n    \n    </div>-->\n    <div class=\"result-list-container\"></div>\n\n    <!-- This section defines results that should be returned based on the search -->\n    <div class=\"results-container\">\n      <spinner v-if=\"showSpinner\"></spinner>\n      <div v-if=\"results && results.length > 0\">\n        <h2>{{ results.length }} Results</h2>\n\n        <ul class=\"results\">\n          <transition-group name=\"fade\" tag=\"div\" appear>\n            <div v-for=\"(result,index) in results\" class=\"breed\" :key=\"index\">\n              <img v-bind:src=\"result\" />\n            </div>\n          </transition-group>\n        </ul>\n      </div>\n      <div v-else class=\"no-results\">\n        <h2>No Pictures Found</h2>\n        <p>Please adjust your search.</p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nrequire(\"vue2-animate/dist/vue2-animate.min.css\");\nimport CubeSpinner from \"@/components/CubeSpinner\";\nimport MessageContainer from \"@/components/MessageContainer\";\nexport default {\n  name: \"breed\",\n  components: {\n    spinner: CubeSpinner,\n    \"message-container\": MessageContainer,\n  },\n  data() {\n    return {\n      results: null,\n      // wordList: [],\n      breed: \"\",\n      breedli: [\n        { val: \"pug\", dog: \"Pug\" },\n        { val: \"pekinese\", dog: \"Pekinese\" },\n        { val: \"australian\", dog: \"Australian Shepherd\"}\n      ],\n      url: \"\",\n      messages: [],\n      status: \"\",\n      showSpinner: false,\n      apiKey: \"26f70159-b7e5-43fd-bb7a-a7669a0072f8\",\n    };\n  },\n  /*fetchImages () {\n    return axios({\n      method: 'get',\n      url: 'https://dog.ceo/api/breed/hound/images/random/10',\n      params: [],\n      showSpinner: false\n    })\n  },*/\n  methods: {\n    findBreed: function () {\n      this.showSpinner = true;\n      this.results = null;\n      this.url =\n        \"https://dog.ceo/api/breed/\" + this.breed + \"/images/random/10\";\n      axios({\n        url: this.url,\n        method: \"get\",\n      })\n        .then((response) => {\n          this.showSpinner = false;\n          this.results = response.data.message;\n        })\n        .catch((error) => {\n          this.showSpinner = false;\n          this.messages.push({\n            type: \"error\",\n            text: error.message,\n          });\n        });\n    },\n  },\n};\n</script>\n\n<style scoped>\nbutton {\n  background: #333;\n  padding: 0.5rem;\n  font-weight: 300;\n  color: #fff;\n  border: none;\n  cursor: pointer;\n  font-size: 1.4rem;\n  border-radius: 0;\n}\n\nh1,\nh2 {\n  font-weight: normal;\n  text-align: center;\n}\n\n.results li {\n  text-align: left;\n  display: inline-block;\n  margin: 10px;\n  border: solid 1px #333;\n  padding: 0.5rem;\n  width: 300px;\n  min-height: 150px;\n  color: gold;\n  font-weight: 300;\n  font-size: 1.2rem;\n  background: black;\n}\n.link {\n  text-align: center;\n}\n\na {\n  color: white;\n}\nimg {\n   width:100%; \n}\n</style>"]}]}